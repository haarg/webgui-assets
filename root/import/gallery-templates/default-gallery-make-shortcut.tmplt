<div class="wgGallery">	
	<div class="wgGalleryBar">	
		<div class="buttons">
			<tmpl_if canAddFile><img src="^FileUrl(root/import/gallery-templates/images/bar-btn-l.jpg);" alt="" /><a href="<tmpl_var url_addAlbum>">^International(template url_addAlbum,Asset_Gallery);</a><img src="^FileUrl(root/import/gallery-templates/images/bar-btn-r.jpg);" alt="" /></tmpl_if>
			<img src="^FileUrl(root/import/gallery-templates/images/bar-btn-l.jpg);" alt="" /><a href="<tmpl_var url>?func=search">Advanced Search</a><img src="^FileUrl(root/import/gallery-templates/images/bar-btn-r.jpg);" alt="" />
			<img src="^FileUrl(root/import/gallery-templates/images/bar-btn-l.jpg);" alt="" /><a href="<tmpl_var url>?func=listFilesForUser">My Gallery</a><img src="^FileUrl(root/import/gallery-templates/images/bar-btn-r.jpg);" alt="" />
			<a href="<tmpl_var url_listAlbumsRss>" class="rss"><img src="^FileUrl(root/import/gallery-templates/images/rss.gif);" style="border-style:none;display:block" /></a>				
		</div>	
		<div class="title"><a href="<tmpl_var url>"><tmpl_var title></a></div>			
	</div>	
	
	<div class="wgSecondaryBar">	
		<div class="searchArea">			
			<form action="<tmpl_var gallery_url>" enctype="multipart/form-data" method="GET" class="searchBox">
				<input type="hidden" name="func" value="search"  />
				<img src="^FileUrl(root/import/gallery-templates/images/search-field-l.jpg);" alt="" />
				<input id="basicSearch_formId" type="text" name="basicSearch" value="" maxlength="255" class="searchText"  />
				<img src="^FileUrl(root/import/gallery-templates/images/search-field-r.jpg);" alt="" />
				<img src="^FileUrl(root/import/gallery-templates/images/sub-btn-l.jpg);" alt="" />
				<input type="submit" name="submit" id="submit_formId" value="Search" class="searchBtn" />
				<img src="^FileUrl(root/import/gallery-templates/images/sub-btn-r.jpg);" alt="" />					
			</form>
		</div>	
		^International(template makeShortcut title,Asset_Photo);
	</div>		

	<br /><br />

	<div id="adminWrapper" style="text-align:left">		
			
		<tmpl_var form_start>
		<table class="adminTable" style="width:350px" cellpadding="5">
		<tr>
			<td class="label">^International(template makeShortcut file,Asset_Photo);</td>
			<td class="data"><tmpl_var title></td>
		</tr>
		<tr>
			<td class="label">^International(template makeShortcut album,Asset_Photo);</td>
			<td class="data"><tmpl_var form_parentId></td>
		</tr>	
		</table>

		<div style="text-align:right;width:350px;margin-top:5px">	
			<input type="button" value="^International(cancel,Asset_GalleryAlbum);" class="adminButton" onclick="history.go(-1); return false" /> <input type="submit" value="^International(save,Asset_GalleryAlbum);" class="adminButton" />
		</div>

		</div></form>
	</div>
</div>

