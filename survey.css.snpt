	content: "Q: ";
}

#survey .scale:before {
	content: "A: ";
}

#survey #submitbutton {
        margin-left: 5px;
}

#restartMessage {
    color: #FF0000;
}

#chart {
    float: left;
    width: 200px;
    height: 113px;
}
