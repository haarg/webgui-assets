/* By default the marker for invalid (required) fields is a red '*' */  
.survey-invalid-marker {
	color: #FF0000;
}


#survey {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	border: 3px solid #1e1e1e;
}

